extends layout/main.pug


include ../blocks/sContent/_sContent.pug
include ../blocks/sContact/_sContact.pug
include ../blocks/sStepNav/_sStepNav.pug
include ../blocks/sSteps/_sSteps.pug

block variables
	- let  mainPage= null
block title
	- let  title= 'Шаг 2: Выбери интерактивный блок'
	title= title
block pagecontent

	//- +mob-mnu()
	+top-nav()
	//- +headerBlock()
	+sStepNavBody()
		.d-none.d-md-block
			.row
				+navItem(`1`,`Выбери <br> обложку планера`)
				+navItem(`2`,`Выбери  <br> главный блок`).active
				+navItem(`3`,`Выбери блок планирования`)
				+navItem(`4`,`Выбери дополнительный блок`)
		.d-md-none
			+b.step-nav-item(href="#").active
				+e.step шаг 2 из 4
				+e.title Выбери  главный блок
	+sSteps2
	+footer('btn')


	